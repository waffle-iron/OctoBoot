{
  "name": "octoboot",
  "version": "0.0.1",
  "description": "OctoBoot is a fast, powerful, and user friendly tools to create your own website in a few clic, all free !! (with help of some free/custom template and gh-pages)",
  "main": "server.js",
  "scripts": {
    "test": "npm start",
    "build": "npm run build:ts && npm run build:handlebars",
    "build:semantic": "cd semantic && gulp build && cd ..",
    "build:ts": "tsc src/front/OctoBoot.ts --removeComments --sourceMap --out dist/octoboot.js",
    "build:handlebars": "handlebars --min src/front/view/*.hbs -f dist/handlebars_templates.min.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RonanDrouglazet/OctoBoot.git"
  },
  "keywords": [
    "gh-pages",
    "website",
    "create"
  ],
  "author": "RonanDrouglazet",
  "license": "GPL",
  "bugs": {
    "url": "https://github.com/RonanDrouglazet/OctoBoot/issues"
  },
  "homepage": "https://github.com/RonanDrouglazet/OctoBoot#readme",
  "dependencies": {
    "better-console": "^0.2.4",
    "cookie-parser": "^1.3.4",
    "cookie-session": "^1.1.0",
    "del": "^1.1.1",
    "express": "^4.12.3",
    "extend": "^2.0.1",
    "github-api": "git+https://github.com/RonanDrouglazet/github-api.git",
    "github-cli": "git+https://github.com/RonanDrouglazet/github-cli.git",
    "gulp": "^3.8.11",
    "gulp-autoprefixer": "^2.2.0",
    "gulp-chmod": "^1.2.0",
    "gulp-clone": "^1.0.0",
    "gulp-concat": "^2.5.2",
    "gulp-concat-css": "^2.2.0",
    "gulp-copy": "0.0.2",
    "gulp-flatten": "0.0.4",
    "gulp-header": "^1.2.2",
    "gulp-help": "^1.3.4",
    "gulp-if": "^1.2.5",
    "gulp-less": "^3.0.3",
    "gulp-minify-css": "^0.5.1",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^0.6.6",
    "gulp-print": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.3",
    "gulp-rtlcss": "^0.1.4",
    "gulp-uglify": "^1.2.0",
    "gulp-util": "^3.0.4",
    "gulp-watch": "^4.2.4",
    "handlebars": "^3.0.3",
    "require-dot-file": "^0.4.0",
    "semantic-ui": "^1.12.2",
    "socket.io": "^1.3.5",
    "typescript": "^1.4.1"
  }
}
