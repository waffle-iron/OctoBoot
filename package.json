{
  "name": "octoboot",
  "version": "0.0.1",
  "description": "OctoBoot is a fast, powerful, and user friendly tools to create your own website in a few clic, all free !! (with help of some free/custom template and gh-pages)",
  "main": "server.js",
  "scripts": {
    "test": "npm start",
    "build": "npm run build:ts && npm run build:handlebars",
    "watch": "parallelshell 'npm run watch:ts' 'npm run watch:handlebars'",
    "build:semantic": "cd semantic && gulp build && cd ..",
    "build:ts": "tsc src/front/OctoBoot.ts --removeComments --sourceMap --out dist/octoboot.js",
    "watch:ts": "watch 'date +\"%m-%d-%Y %T\" && npm run build:ts' src/front/",
    "build:handlebars": "handlebars --min src/front/view/*.hbs -f dist/handlebars_templates.min.js",
    "watch:handlebars": "watch 'date +\"%m-%d-%Y %T\" && npm run build:handlebars' src/front/view/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RonanDrouglazet/OctoBoot.git"
  },
  "keywords": [
    "gh-pages",
    "website",
    "create"
  ],
  "author": "RonanDrouglazet",
  "license": "GPL",
  "bugs": {
    "url": "https://github.com/RonanDrouglazet/OctoBoot/issues"
  },
  "homepage": "https://github.com/RonanDrouglazet/OctoBoot#readme",
  "dependencies": {
    "better-console": "^0.2.4",
    "cookie-parser": "^1.3.4",
    "cookie-session": "^1.1.0",
    "del": "^2.0.0",
    "express": "^4.12.3",
    "extend": "^3.0.0",
    "github-api": "git+https://github.com/RonanDrouglazet/github-api.git",
    "github-cli": "git+https://github.com/RonanDrouglazet/github-cli.git",
    "gulp": "^3.9.0",
    "gulp-autoprefixer": "^2.3.1",
    "gulp-chmod": "^1.2.0",
    "gulp-clone": "^1.0.0",
    "gulp-concat": "^2.6.0",
    "gulp-concat-css": "^2.2.0",
    "gulp-copy": "0.0.2",
    "gulp-flatten": "^0.2.0",
    "gulp-header": "^1.7.1",
    "gulp-help": "^1.6.1",
    "gulp-if": "^1.2.5",
    "gulp-less": "^3.0.3",
    "gulp-minify-css": "^1.2.1",
    "gulp-notify": "^2.2.0",
    "gulp-plumber": "^1.0.1",
    "gulp-print": "^1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-replace": "^0.5.4",
    "gulp-rtlcss": "^0.1.4",
    "gulp-uglify": "^1.4.0",
    "gulp-util": "^3.0.6",
    "gulp-watch": "^4.3.5",
    "handlebars": "^3.0.3",
    "map-stream": "0.0.6",
    "parallelshell": "latest",
    "require-dot-file": "^0.4.0",
    "semantic-ui": "2.1.3",
    "socket.io": "^1.3.5",
    "typescript": "^1.4.1",
    "watch": "latest",
    "yamljs": "^0.2.3"
  }
}
