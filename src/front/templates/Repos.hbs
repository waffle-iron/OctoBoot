{{!-- data context -> Array<model.GitHubRepo> --}}
<div class="item Repos">
    <b class="{{bind on}}" style="cursor: pointer">{{title}} project</b> <span class="ui label">{{repos.length}}</span>
    <div class="menu" style="display: none">
    {{#each repos}}
      <a class="item">
        {{name}}
      </a>
    {{/each}}
    </div>
</div>
